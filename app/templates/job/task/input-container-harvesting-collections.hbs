
<AuDataTable
  @content={{this.model}}
  @noDataMessage="No remote-data-objects found"
  @sort={{this.sort}}
  @size={{this.size}}
  @page={{this.page}} as |t|>
  <t.content as |c|>
    <c.header>
      <th>URI</th>
      <th>Source (URL)</th>
    </c.header>
    <c.body as |row|>
      <td>
        {{row.uri}}
      </td>
      <td>
        {{row.source}}
      </td>
    </c.body>
  </t.content>
</AuDataTable>
