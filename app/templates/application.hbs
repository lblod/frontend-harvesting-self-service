{{page-title "Harvesting self service"}}

<AuToaster @position="top"/>
<AuModalContainer />
<AuApp>
  <AuMainHeader
    @appTitle="Harvesting self service"
    @brandLink="https://www.vlaanderen.be/nl"
    @homeRoute="index">
    {{#if this.authenticationEnabled}}
      {{#if this.session.isAuthenticated}}
        <MuLogout />
      {{/if}}
    {{/if}}
  </AuMainHeader>
  <AuToolbar
    @size="small"
    @skin="tint"
    @border="bottom"
    as |Group|>
    <Group>
      <Breadcrumb @breadcrumbs={{this.breadcrumbs}} />
    </Group>
  </AuToolbar>
  {{outlet}}
</AuApp>
